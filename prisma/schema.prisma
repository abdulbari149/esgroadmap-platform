model User {
  id Int @unique(map: "id") @default(autoincrement())

  username     String  @db.VarChar(255)
  email        String  @db.VarChar(255)
  password     String  @db.VarChar(255)
  isActive     Boolean @map("is_active") @db.TinyInt
  profileImage String  @default("") @map("profile_image") @db.Text
  plan         Int     @default(1) @map("plan") @db.Int
  role         String  @default("user") @db.VarChar(255)
  stripeId     String? @default("stripe_id") @db.VarChar(255)

  createdAt        DateTime           @default(now()) @map("created_at")
  updatedAt        DateTime?          @map("updated_at")
  deletedAt        DateTime?          @map("deleted_at")
  tickets          Ticket[]
  ticketComments   TicketComment[]
  UserSubscription UserSubscription[]
  Portfolio        Portfolio[]

  @@map("users")
}

model Ticket {
  id Int @unique(map: "id") @default(autoincrement())

  userId   Int  @db.Int
  postedBy User @relation(fields: [userId], references: [id])

  title       String @db.VarChar(255)
  description String @db.Text
  status      String @db.VarChar(255)

  createdAt DateTime         @default(now()) @map("created_at")
  updatedAt DateTime?        @map("updated_at")
  deletedAt DateTime?        @map("deleted_at")
  documents TicketDocument[]
  comments  TicketComment[]

  @@map("tickets")
}

model TicketDocument {
  id Int @unique(map: "id") @default(autoincrement())

  url  String
  name String
  size Int

  createdAt DateTime  @default(now()) @map("created_at")
  updatedAt DateTime? @map("updated_at")
  deletedAt DateTime? @map("deleted_at")
  ticketId  Int
  ticket    Ticket    @relation(fields: [ticketId], references: [id])

  @@map("ticket_documents")
}

model TicketComment {
  id Int @unique(map: "id") @default(autoincrement())

  text     String @db.Text
  ticketId Int
  ticket   Ticket @relation(fields: [ticketId], references: [id])

  userId   Int  @db.Int
  postedBy User @relation(fields: [userId], references: [id])

  createdAt DateTime  @default(now()) @map("created_at")
  updatedAt DateTime? @map("updated_at")
  deletedAt DateTime? @map("deleted_at")

  @@map("ticket_comments")
}

model UserSubscription {
  id Int @unique(map: "id") @default(autoincrement())

  userId Int  @db.Int
  user   User @relation(fields: [userId], references: [id])

  subscriptionId String @map("subscription_id")

  createdAt DateTime  @default(now()) @map("created_at")
  updatedAt DateTime? @map("updated_at")
  deletedAt DateTime? @map("deleted_at")

  @@map("user_subscriptions")
}

model Portfolio {
  id Int @unique(map: "id") @default(autoincrement())

  userId    Int    @db.Int
  user      User   @relation(fields: [userId], references: [id])
  name      String @db.VarChar(255)
  filters   Json   @db.Json
  tableName String @map("table_name") @db.VarChar(255)

  createdAt DateTime  @default(now()) @map("created_at")
  updatedAt DateTime? @map("updated_at")
  deletedAt DateTime? @map("deleted_at")

  @@map("portfolios")
}
